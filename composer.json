{
  "name": "pluggit/logging",
  "description": "Logging provides a thin PSR-7 interface abstraction to connect any logging facility behind it.",
  "require": {
    "php": "^5.4|^7.0",
    "psr/log": "^1.0"
  },
  "require-dev": {
    "monolog/monolog": "^1.23",
    "phpspec/phpspec": "^2.5",
    "behat/behat": "^3.1",
    "phpunit/phpunit": "4.8.*",
    "squizlabs/php_codesniffer": "2.*",
    "henrikbjorn/phpspec-code-coverage": "2.*"
  },
  "license": "MIT",
  "config": {
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-4": {
      "Cmp\\Logging\\": "src/",
      "features\\Cmp\\Logging\\": "tests/features/bootstrap/"
    }
  },
  "authors": [
    {
      "name": "jaroslawgabaracmp",
      "email": "jaroslaw.gabara@teamcmp.com"
    }
  ]
}
